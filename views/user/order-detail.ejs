                    customClass: {
                        popup: 'custom-swal-popup'
                    }
                });
                return;
            }

            const response = await fetch(`/returnOrder?id=<%= order._id %>&allProduct=${value}`, {
                method:'POST',
                headers: {
                    'content-type': 'application/json'
                },
                body: JSON.stringify({reason})
            });
            const data = await response.json();
            if(!response.ok) {
